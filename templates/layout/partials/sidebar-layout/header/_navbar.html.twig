<!--begin::Navbar-->
<div class="app-navbar flex-shrink-0">
	<!--begin::Notifications-->
	<div class="app-navbar-item ms-1 ms-md-4">
		<!--begin::Menu wrapper-->
		<div class="btn btn-icon btn-custom btn-icon-muted btn-active-light btn-active-color-primary w-35px h-35px" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-attach="parent" data-kt-menu-placement="bottom-end" id="kt_menu_item_wow">
			{{ theme.getIcon('notification-status', 'fs-2') | raw }}
		</div>
		{% include "partials/menus/_notifications-menu.html.twig" %}
		<!--end::Menu wrapper-->
	</div>
	<!--end::Notifications-->

	<!--begin::User menu-->
	<div class="app-navbar-item ms-1 ms-md-4" id="kt_header_user_menu_toggle">
		<!--begin::Menu wrapper-->
		<div class="cursor-pointer symbol symbol-35px" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-attach="parent" data-kt-menu-placement="bottom-end">
			{% if app.user %}
				<span class="symbol-label bg-primary text-inverse-primary fw-bold">{{ app.user.firstName|slice(0, 1)|upper }}</span>
			{% else %}
				<span class="symbol-label bg-secondary text-inverse-secondary fw-bold">?</span>
			{% endif %}
		</div>
		{% include "partials/menus/_user-account-menu.html.twig" %}
		<!--end::Menu wrapper-->
	</div>
	<!--end::User menu-->

	<!--begin::Header menu toggle-->
	<div class="app-navbar-item d-lg-none ms-2 me-n2" title="Menüyü göster">
		<div class="btn btn-flex btn-icon btn-active-color-primary w-30px h-30px" id="kt_app_header_menu_toggle">{{ theme.getIcon('element-4', 'fs-1') | raw }}</div>
	</div>
	<!--end::Header menu toggle-->
</div>
<!--end::Navbar-->
